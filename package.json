{
  "name": "medic",
  "description": "Self-healing automation",
  "version": "0.0.0",
  "homepage": "",
  "main": "src/index",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Ryan Wheale",
    "email": "ryan.wheale@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0 <9.0.0",
    "npm": ">= 3.0.0"
  },
  "steal": {
    "main": "public/index",
    "map": {
      "@root": ".",
      "@public": "~/public",
      "styled-components": "styled-components/dist/styled-components",
      "polished": "polished/dist/polished",
      "socket.io-client": "socket.io-client/dist/socket.io"
    },
    "plugins": [
      "steal-css",
      "steal-less",
      "systemjs-plugin-text"
    ],
    "configDependencies": [
      "stealjs/env-config"
    ],
    "babelOptions": {
      "plugins": [
        "transform-class-properties"
      ]
    }
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha && npm run testee",
    "eslint": "eslint src/. test/. --config .eslintrc.json && npm run eslint-public",
    "eslint-public": "eslint public/. --config .eslintrc-public.json",
    "db-migrate": "db-migrate up --config ./migrations/config.js -m ./migrations/migrations",
    "prestart": "npm run db-migrate",
    "start": "node src/",
    "mocha": "NODE_ENV=test mocha test/ --require=dotenv/config --recursive",
    "testee": "testee ./public/test.html -c .testee.js",
    "build": "NODE_ENV=production node scripts/build",
    "predev-bundle": "node scripts/semantic-ui-setup",
    "dev-bundle": "steal-tools bundle --dev --minify=false --dest public/",
    "develop": "npm run prestart && nodemon -r dotenv/config src/",
    "debug": "npm run prestart && nodemon -r dotenv/config --inspect src/",
    "build-commit": "git branch -D dist; git checkout -b dist && npm run build && git add public/dist -f && git commit -m 'Update dists for deploy'",
    "deploy-prod": "npm run build-commit && git push prod dist:master -f; git checkout -",
    "postinstall": "if [ -z \"$npm_config_production\" ]; then npm run dev-bundle; fi",
    "jsdoc": "node ./scripts/jsdoc",
    "storybook": "npm run jsdoc && start-storybook -p 9001 -c storybook"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "case": "^1.5.4",
    "compression": "^1.7.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csurf": "^1.9.0",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-pg": "^0.2.5",
    "debug": "^3.1.0",
    "feathers": "^2.2.2",
    "feathers-authentication": "^1.2.7",
    "feathers-authentication-custom": "feathersjs-ecosystem/feathers-authentication-custom#build-test",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-authentication-jwt": "^0.3.2",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.9.2",
    "feathers-hooks": "^2.1.0",
    "feathers-hooks-common": "^3.10.0",
    "feathers-logger": "^0.2.3",
    "feathers-memory": "^1.2.1",
    "feathers-rest": "^1.8.0",
    "feathers-sequelize": "^2.4.0",
    "feathers-socketio": "^2.0.0",
    "helmet": "^3.8.2",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "pg": "^7.4.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.22.11",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-info": "^3.2.15",
    "@storybook/addon-knobs": "^3.3.3",
    "@storybook/react": "^3.2.14",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "can-ajax": "^1.1.0",
    "can-connect": "^2.0.0-pre.16",
    "can-connect-feathers": "^4.0.0-pre.0",
    "can-define": "^2.0.0-pre.24",
    "can-fixture": "^2.0.0-pre.2",
    "can-fixture-socket": "^0.7.2",
    "can-route-pushstate": "^4.0.0-pre.4",
    "can-set": "^1.4.0",
    "can-stream-kefir": "^1.0.0-pre.0",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.5.1",
    "feathers-client": "^2.4.0",
    "find-in-files": "^0.4.0",
    "generator-rvm-component":"git+ssh://git@github.pie.apple.com:icloud-automation-sre/generator-rvm-component.git#0.1.3",
    "image-webpack-loader": "^3.4.2",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "ncp": "^2.0.0",
    "nodemon": "^1.12.1",
    "polished": "^1.8.1",
    "prop-types": "^15.6.0",
    "puppeteer": "^0.13.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-d3-tree": "^1.5.0",
    "react-dom": "^16.0.0",
    "react-json-view": "^1.13.1",
    "react-storybook-addon-chapters": "^2.1.1",
    "react-view-model": "^1.0.0-pre.9",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "semantic-ui-less": "^2.2.11",
    "semantic-ui-react": "^0.75.1",
    "socket.io-client": "^2.0.3",
    "steal": "^1.6.2",
    "steal-css": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.9.1",
    "strip-comments": "^0.4.4",
    "styled-components": "^2.2.3",
    "systemjs-plugin-text": "0.0.11",
    "testee": "^0.7.0"
  }
}
